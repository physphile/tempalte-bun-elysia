{
  "name": "template-bun-elysia",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "auth:generate": "bunx --bun @better-auth/cli@1.3.4 generate --output src/database/schemas/auth.ts",
    "db:up": "docker run --rm -d -p 5433:5432 -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=postgres postgres"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/swagger": "^1.3.1",
    "better-auth": "1.3.7",
    "drizzle-orm": "^0.44.5",
    "drizzle-typebox": "^0.3.3",
    "elysia": "^1.3.20"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "bun-types": "^1.2.21",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "pg": "^8.16.3"
  },
  "overrides": {
    "@sinclair/typebox": "0.34.33"
  },
  "module": "src/index.js",
  "patchedDependencies": {
    "better-auth@1.3.7": "patches/better-auth@1.3.7.patch"
  }
}
